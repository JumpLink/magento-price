{
  "main": "app/splashscreen.html",
  "name": "magento-desktop",
  "description": "Manage your Magento Shop",
  "version": "0.0.1",
  "dependencies": {
    "nw-gyp": "*",
    "async": "~0.2.9",
    "asyncblock": "*",
    "magento": "JumpLink/node-magento",
    "libnotify": "*",
    "nedb": "~0.8.6",
    "q": "~0.9.6",
    "less": "~1.4.2",
    "mkdirp": "~0.3.5",
    "nwglobal": "0.0.1"
  },
  "keywords": [
    "Magento",
    "Desktop"
  ],
  "window": {
    "title": "Magento Desktop",
    "icon": "app/images/magento-logo.png",
    "fullscreen": false,
    "toolbar": false,
    "frame": false,
    "width": 500,
    "height": 200,
    "position": "center",
    "always-on-top": true,
    "show": false
  },
  "webkit": {
    "plugin": true
  }
}
