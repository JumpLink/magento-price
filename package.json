{
  "main": "app/splashscreen.html",
  "name": "Magento Desktop",
  "description": "Manage your Magento Shop",
  "version": "0.0.1",
  "dependencies": {
  	"nw-gyp": "*",
    "async": "*",
    "asyncblock": "*",
    "magento": "JumpLink/node-magento",
    "libnotify": "*"
  },
  "keywords": [ "Magento", "Desktop" ],
  "window": {
    "title": "Magento Desktop",
    "icon": "app/images/magento-logo.png",
    "fullscreen": false,
    "toolbar": false,
    "frame": false,
    "width": 500,
    "height": 200,
    "position": "center",
    "always-on-top": true,
    "show": false
  },
  "webkit": {
    "plugin": true
  }
}